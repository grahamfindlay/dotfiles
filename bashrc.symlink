alias clean='rm -rf ./*~; rm -rf ./*#; rm -rf ./*.orig'
alias h='history'
alias lf="ls -l | grep -v '^d1;2c'" # just files
alias ldir="ls -l | grep '^d'" # just directories

# Set prompt, ex: [~][master]$ your_command_here
export PS1="[\W][\$(git branch 2>/dev/null | grep '^*' | colrm 1 2)]\$ " 

# use vim keybindings
set -o vi

